<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/firstFragment2"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/firstFragment2"
        android:name="com.example.navigationcomponent.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment2_to_redFragment2"
            app:destination="@id/redFragment2" />
        <action
            android:id="@+id/action_firstFragment2_to_blueFragment2"
            app:destination="@id/blueFragment2" />
        <action
            android:id="@+id/action_firstFragment2_to_greenFragment2"
            app:destination="@id/greenFragment2" />
    </fragment>
    <fragment
        android:id="@+id/redFragment2"
        android:name="com.example.navigationcomponent.RedFragment"
        android:label="fragment_red"
        tools:layout="@layout/fragment_red" />
    <fragment
        android:id="@+id/blueFragment2"
        android:name="com.example.navigationcomponent.BlueFragment"
        android:label="fragment_blue"
        tools:layout="@layout/fragment_blue" />
    <fragment
        android:id="@+id/greenFragment2"
        android:name="com.example.navigationcomponent.GreenFragment"
        android:label="fragment_green"
        tools:layout="@layout/fragment_green" />
</navigation>